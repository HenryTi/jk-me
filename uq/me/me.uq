ID Account (
	id,
	KEY user ID,
	sys (create, owner),
);

ID AccountType (
	id,
	key type char(50),				-- 'cto', ...
);

IX UserAccountType (
	ix User,
	xi AccountType,
);

IDX AccountCTO (
	id,
	commissionFormula ID $Piecewise,			-- 对应的分段计算提成公式
	orderAmount DEC(16,4) SUM LOG,				-- 下订单
	orderDeliver DEC(16,4) SUM LOG,				-- 发货
	orderPaid DEC(16,4) SUM LOG,				-- 收款
	amount DEC(16,4) SUM LOG,					-- 实际可提金额
);
